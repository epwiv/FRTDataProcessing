Project ID: Open PAF Visual3D Example
Package Information:
  Name: Visual3D Example
  Version: 1.0.0
  Required QTM version: 2.16.3600
Root type: Subject
Default fields: [Creation date, Creation time]

Date format: Iso
Date separator: Dash
Filename filter: \.(pdf|r3w|cmo|docx)$

Types:
  Subject:
    Participant:
      Fields: [ID, Height, Weight, Leg]
      Children: [Session]
      Directory pattern: $ID$

  Session:
    Session:
      Fields: [Session ID, Operator]
      Measurements: [Static, Pointer, Overground, No_Resistance, Training, Catch]
      Analyses: [Processing2]
      Display order: [Session ID, Operator, Comments]
      Directory pattern: $Session ID$

Measurements:
  Fields:
    Measurement length: 30
    Minimum count: 1
    Maximum count: 1
    Count: 1
  Static:
    Heading: Static
    Measurement length: 10
    Measurement type: Static
    AIM models: Static.qam
    Maximum count: 4
    Minimum count: 2
    Count: 2
  Pointer:
    Heading: Pointer
    Measurement length: 60
    Measurement type: Static
    AIM models: Static.qam
    Maximum count: 4
    Minimum count: 2
    Count: 2
  Overground:
    Heading: Overground
    Measurement length: 20
    Maximum count: 4
    Minimum count: 2
    Count: 2
    Measurement type: Dynamic
    AIM models: Dynamic.qam
  No_Resistance:
    Heading: No_Resistance
    Measurement length: 120
    Maximum count: 3
    Count: 1
    Measurement type: Dynamic
    AIM models: Dynamic.qam
  Training:
    Heading: Training
    Measurement length: 300
    Maximum count: 7
    Minimum count: 4
    Count: 5
    Measurement type: Dynamic
    AIM models: Dynamic.qam
  Catch:
    Heading: Catch
    Measurement length: 30
    Maximum count: 6
    Minimum count: 3
    Count: 4
    Measurement type: Dynamic
    AIM models: Dynamic.qam2

Analyses:
  Processing:
    Type: External program
    Program display name: Visual 3d
    Export session: Yes
    Export measurements: c3d
    Arguments: [/s, $WorkingDirectory$combine_output.v3s]
    Template files: ['*.v3s']
  Processing2:
    Type: External program
    Program display name: Visual 3d
    Export session: Yes
    Export measurements: c3d
    Arguments: [/s, $WorkingDirectory$combine_output.v3s]
    Template files: ['*.v3s']

Fields:
  ID:
    Type: String
    Inherit: Connect
  Name:
    Type: String
    Inherit: Connect
    Force: Yes
  DOB:
    Type: Date
    Inherit: Connect
  Leg:
    Type: Enum
    Values: [Left, Right]
    Inherit: Connect
    Force: Yes
  Creation date:
    Type: Date
    Readonly: Yes
  Creation time:
    Type: TimeOfDay
    Readonly: Yes
  Session ID:
    Type: Enum
    Values: [ECB, ELASTIC, HIP KNEE]
    Inherit: Connect
  Operator:
    Type: String
    Inherit: Copy
  Height:
    Type: Float
    Quantity: Length
    Unit: centimeters
    Decimals: 0
    Inherit: Connect
    Max: 250
    Min: 50
  Weight:
    Type: Float
    Quantity: Mass
    Unit: kilograms
    Decimals: 1
    Inherit: Copy
    Max: 250
    Min: 0
  Sex:
    Type: Enum
    Values: [Female, Male, Other]
    Inherit: Connect
  Measurement type:
    Type: String

Columns:
  Default column definitions:
    ID:
      Width: 200
      Fields:
        Default: Filename
        Participant: ID
    Name:
      Width: 150
      Fields: 
        Participant: Name
    Created:
      Width: 65
      Field: Creation date
    Comments:
      Field: Comments
      Width: 100